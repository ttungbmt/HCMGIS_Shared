{
    "name": "larabase/nova-base",
    "description": "A wonderful administration interface for Laravel.",
    "keywords": [
        "laravel",
        "admin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Truong Thanh Tung",
            "email": "ttungbmt@gmail.com"
        }
    ],
    "require": {
        "digital-creative/collapsible-resource-manager": "^1.3",
        "eminiarts/nova-tabs": "^1.4",
        "ebess/advanced-nova-media-library": "^3.6",
        "maatwebsite/laravel-nova-excel": "^1.2",
        "titasgailius/search-relations": "^1.0",
        "whitecube/nova-flexible-content": "^0.2.8",
        "optimistdigital/nova-translations-loader": "^3.1",
        "codenco-dev/nova-grid-system": "^1.0",
        "larabase/nova-theme": "*@dev",
        "degecko/nova-filters-summary": "*@dev",
        "vyuldashev/nova-permission": "*@dev"
    },
    "autoload": {
        "psr-4": {
            "Larabase\\Nova\\": "src",
            "Larabase\\NovaDatabase\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [

            ],
            "providers": [
                "Larabase\\Nova\\NovaServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ttungbmt/nova-base"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "0.0.1"
}
