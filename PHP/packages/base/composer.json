{
  "name": "larabase/base",
  "description": "A wonderful administration interface for Laravel.",
  "keywords": [
    "laravel",
    "admin"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Truong Thanh Tung",
      "email": "ttungbmt@gmail.com"
    }
  ],
  "require": {
      "rennokki/laravel-eloquent-query-cache": "^2.6",
      "larabase/laravel-macros": "*@dev",
      "owen-it/laravel-auditing": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Larabase\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Larabase\\Providers\\AppServiceProvider",
        "Larabase\\Providers\\ShortcodeServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "version": "0.0.1"
}
