{
  "name": "@ttungbmt/vue-leaflet",
  "version": "0.0.1",
  "description": "Leaflet for Vue.js",
  "main": "dist/vue-leaflet.js",
  "scripts": {
    "clean": "rm -rf dist/*",
    "watch": "bili -w",
    "watch:push": "run-p watch push:watch",
    "build": "bili",
    "build:push": "run-s build push",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --list-different \"src/**/*.js\" \"docs/*/**.md\"",
    "packages:upgrade": "yarn upgrade-interactive --latest",
    "push": "yalc push",
    "push:watch": "nodemon --watch dist --delay 1000ms -x yarn push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ttungbmt/vue-leaflet.git"
  },
  "author": "Truong Thanh Tung",
  "license": "MIT",
  "homepage": "https://github.com/ttungbmt/vuex#readme",
  "dependencies": {
    "@geoman-io/leaflet-geoman-free": "^2.9.0",
    "file-saver": "^2.0.5",
    "leaflet": "^1.7.1",
    "leaflet-boundary-canvas": "^1.0.0",
    "leaflet-extra-markers": "^1.2.1",
    "leaflet.browser.print": "^1.0.6",
    "leaflet.fullscreen": "^2.0.0",
    "leaflet.locatecontrol": "^0.73.0",
    "leaflet.polylinemeasure": "^2.0.0",
    "lodash-es": "^4.17.21",
    "screenfull": "^5.1.0",
    "vue": "^2.6.12",
    "vue-runtime-helpers": "^1.1.2",
    "vue2-leaflet": "^2.6.0",
    "vue2-leaflet-draw-toolbar": "^0.1.1"
  },
  "devDependencies": {
    "bili": "^5.0.5",
    "rollup-plugin-vue": "^5.0.1",
    "vue-template-compiler": "^2.6.12"
  }
}
